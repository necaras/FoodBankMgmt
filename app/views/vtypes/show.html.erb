<div class="container">

	<%= render :partial => "home/header" %>

	<div class="main">

		<div class="item">

			<div class="date">
			<div>&nbsp</div>
			<span>&nbsp</span>
			</div>

			<div class="content">
				<h1>Showing Volunteer Type</h1>

				<div class="body"> 
				
					<% if not notice.nil? %>
						<p id="notice"><%= notice %></p>
					<% end %>
					
					<p>
						<b>Description:</b>
						<%= @vtype.description %>
					</p>

					<%= link_to 'Edit', edit_vtype_path(@vtype) %> |
					<%= link_to 'Back', vtypes_path %>
				</div><!--body-->
			</div><!--content-->
		</div><!--item-->
	</div><!--main-->

	<%= render :partial => "home/navigation" %>

	<div class="clearer"><span></span></div>

	<%= render :partial => "home/footer" %>

</div><!--container-->
